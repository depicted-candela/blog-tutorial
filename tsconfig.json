{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "paths": {
      "@pageTypes/*": [
        "./src/types/pages/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}